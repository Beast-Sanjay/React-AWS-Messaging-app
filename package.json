{
  "name": "F13-Hobbies-chat-demo",
  "version": "1.0.1",
  "description": "Demo app for for Amazon Chime Chat SDK",
  "scripts": {
    "lint": "eslint src --ext .js,.jsx --fix",
    "build": "rimraf dist && webpack build",
    "sam:build": "cd src/backend/serverless && rm -rf .aws-sam && sam build && cd -",
    "deploy:amplify": "npm run sam:build && npm run build && cp src/aws-exports.js dist/aws-exports.js && cp src/index.html dist/index.html && cp src/logo.png dist/logo.png && cp src/amplify.js dist/amplify.js",
    "deploy": "npm run deploy:amplify"
  }
,  
